{
  "name": "booted",
  "version": "0.0.1",
  "description": "A newly booted project",
  "main": "./server/index.js",
  "author": "booted",
  "license": "MIT",
  "engines": {
    "node": ">=7.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "browser-sync": "^2.18.8",
    "browserify": "^14.1.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "uglify-js": "^2.8.10",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "run-p start:server watch:scripts watch:styles proxy:browser",
    "start:server": "nodemon ./server/index --watch ./server -e js,json,pug",
    "watch:styles": "stylus --sourcemap-inline --watch ./client/styles/index.styl --out ./public/styles.css",
    "watch:scripts": "watchify ./client/scripts/index.js -o ./public/scripts.js",
    "proxy:browser": "browser-sync start --proxy localhost:3000 --port 3001 --files ./public/* --no-notify",
    "reload:browser": "browser-sync reload --port 3001",
    "clean": "rimraf ./public/*"
  },
  "dependencies": {
    "koa": "^2.1.0",
    "koa-better-error-handler": "^1.0.9",
    "koa-bodyparser": "^4.1.0",
    "koa-conditional-get": "next",
    "koa-convert": "^1.2.0",
    "koa-error": "^3.0.0",
    "koa-etag": "3",
    "koa-helmet": "^3.1.0",
    "koa-logger": "next",
    "koa-methodoverride": "^2.0.0",
    "koa-pug": "^3.0.0-2",
    "koa-route": "^3.2.0",
    "koa-static": "next",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "pretty-error": "^2.0.2",
    "pug": "^2.0.0-beta11"
  }
}
